		<IsPackable>false</IsPackable>
	</PropertyGroup>

	<ItemGroup>
		<None Include="TestData\**" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Build.Locator" Version="1.5.5" />
